<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="crm_lead_view_inherit_form" model="ir.ui.view">
        <field name="name">crm.lead.view.form.inherit</field>
        <field name="model">crm.lead</field>
        <field name="inherit_id" ref="bi_new_customized_fields.inherited_view_crm_lead_new_fields_form"/>
        <field name="arch" type="xml">
            <xpath expr="field[@name='sub_date']" position="after">
                <field name="actual_sub_date"/>
            </xpath>
        </field>
    </record>
</odoo>
