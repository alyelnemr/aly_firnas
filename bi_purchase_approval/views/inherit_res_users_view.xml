<?xml version="1.0"?>
<odoo>
    <data>
        <record id="res_users_approve_form_view" model="ir.ui.view">
            <field name="name">res.users.pos.return.form.view.inherit</field>
            <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
