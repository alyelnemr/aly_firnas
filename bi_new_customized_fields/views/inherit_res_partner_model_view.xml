<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_res_partner_new_fields_form_inherit" model="ir.ui.view">
            <field name="name">view.res.partner.form.inherit</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group/group[2]/field[@name='website']" position="before">
                    <field name="fax" widget="phone"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>